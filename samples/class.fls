class Animal
    ctor(name)
        this.name: name

    func move(x)
        return this.name + ' moved ' + x + ' meters'

class Bird: Animal
    ctor(name, speed) super(name)
        this.speed: speed

    func move(x)
        if this.speed = 1
            return 'Bird ' + super.move(x)
        return 'Bird ' + this.name + ' flied ' + x * this.speed + ' meters'

a: Animal('Alche')
console.log(a.move(5))

b: Bird('Rokh', 2)
console.log(b.move(4))

d: Bird('Duck', 1)
console.log(d.move(3))
